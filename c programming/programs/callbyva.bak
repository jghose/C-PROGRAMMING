#include<stdio.h>
void interchange(int number1,int number2)
{
    int temp;
    temp = number1;
    number1 = number2;
	 number2 = temp;
	 printf("\nNumber 1 : %d",number1);
	 printf("\nNumber 2 : %d",number2);
}
int main() {
	 int num1=50,num2=70;
	 interchange(num1,num2);
	return(0);
}
